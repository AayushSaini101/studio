{
  "name": "@asyncapihub/plugin-billing",
  "version": "0.2.0",
  "description": "Adds billing to AsyncAPI Hub.",
  "main": "index.js",
  "author": "Fran Mendez <fmvilas@gmail.com>",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "asyncapihub": {
    "routes": [
      {
        "urlPath": "/settings/upgrade/session",
        "routeHandlerPath": "src/routes/checkout-session.js",
        "method": "post",
        "session": true
      },
      {
        "urlPath": "/settings/upgrade/subscription",
        "routeHandlerPath": "src/routes/update-subscription.js",
        "method": "put",
        "session": true
      }
    ],
    "pages": {
      "/settings/billing": {
        "session": true,
        "pagePath": "src/pages/billing.js",
        "routeHandlerPath": "src/routes/billing.js"
      },
      "/settings/upgrade": {
        "session": true,
        "pagePath": "src/pages/upgrade.js",
        "routeHandlerPath": "src/routes/upgrade.js"
      }
    },
    "ui": {
      "settings": {
        "user": {
          "navigation": [
            {
              "id": "upgrade",
              "path": "/upgrade",
              "text": "Upgrade"
            }
          ]
        }
      }
    }
  },
  "dependencies": {
    "@stripe/stripe-js": "^1.5.0",
    "react-icons": "^3.10.0",
    "stripe": "^8.51.0"
  }
}
