{
  "name": "@asyncapihub/plugin-waiting-list",
  "version": "0.1.0",
  "description": "Make AsyncAPI Hub implement a waiting list when someone signs up.",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Fran Mendez <fmvilas@gmail.com>",
  "license": "Apache-2.0",
  "asyncapihub": {
    "hooks": {
      "auth:github": [
        "src/hooks/signin.js"
      ]
    },
    "middlewares": [
      {
        "path": "src/hooks/middleware.js",
        "session": false
      }
    ],
    "pages": {
      "/landing/waiting-list": {
        "session": false,
        "pagePath": "src/pages/waiting-list.js",
        "routeHandlerPath": "src/routes/waiting-list.js"
      }
    }
  }
}
